datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

enum Role {
    USER
    ADMIN
}

model User {
    id         Int       @id @default(autoincrement())
    createdAt  DateTime  @default(now())
    email      String    @unique
    name       String?
    role       Role      @default(USER)
    created_at DateTime? @default(now())
}
